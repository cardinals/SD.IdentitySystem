<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <!--依赖注入提供者配置节点-->
    <section name="injectionProviderConfiguration" type="SD.IOC.Core.Configuration.InjectionProviderConfiguration,SD.IOC.Core" />
    <!--依赖注入注册配置节点-->
    <section name="injectionRegisterConfiguration" type="SD.IOC.Core.Configuration.InjectionRegisterConfiguration,SD.IOC.Core" />
    <!--缓存配置节点-->
    <section name="cacheConfiguration" type="SD.CacheManager.Configuration.CacheConfiguration,SD.CacheManager" />
  </configSections>
  <!--依赖注入提供者配置，默认采用Autofac-->
  <injectionProviderConfiguration type="SD.IOC.Core.AutofacProvider.AutofacInstanceResolver" assembly="SD.IOC.Core" />
  <!--WCF配置-->
  <system.serviceModel>
    <!--扩展元素配置-->
    <extensions>
      <!--行为扩展配置-->
      <behaviorExtensions>
        <!--WCF服务端身份认证扩展配置-->
        <add name="ServerAuthentication" type="ShSoft.UAC.WcfAuthentication.Server.AuthenticationBehaviorElement, ShSoft.UAC.WcfAuthentication.Server" />
      </behaviorExtensions>
    </extensions>
  </system.serviceModel>
  <!--缓存配置-->
  <cacheConfiguration type="SD.CacheManager.Redis.Implements.RedisCacheAdapter" assembly="SD.CacheManager.Redis" />
  <!--应用程序配置-->
  <appSettings>
    <!--Redis服务器地址，示例：127.0.0.1,6379-->
    <add key="RedisServer" value="192.168.3.23,6379" />
  </appSettings>
</configuration>